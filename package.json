{
  "name": "visualization-editor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev src/docs",
    "docs:build": "vitepress build src/docs",
    "docs:serve": "vitepress serve src/docs",
    "prepare": "husky install",
    "test:unit": "vitest run"
  },
  "main": "./index.ts",
  "dependencies": {
    "add": "^2.0.6",
    "husky": "^8.0.3",
    "sass": "^1.58.0",
    "scss": "^0.2.4",
    "vitest": "0.34.6",
    "vue": "^3.2.45",
    "vue-grid-layout": "3.0.0-beta1"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.56.1",
    "@arco-plugins/vite-vue": "^1.4.5",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/node": "^18.15.3",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "lint-staged": "^15.2.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vitepress": "^1.0.0-alpha.49",
    "vue-tsc": "^1.0.24"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,vue}": [
      "npm run test"
    ]
  }
}
